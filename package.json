{
  "name": "deno-require",
  "version": "1.0.0",
  "description": "You can use this module to require a module in node just like deno",
  "main": "index.js",
  "directories": {
    "example": "example",
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-files": "copyfiles ./pkg/* ./example/pkg",
    "example": "webpack-dev-server --config example/webpack.config.js",
    "serve": "run-s copy-files example",
    "build": "scripts/wasm/build.sh --features plugin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TyrealHu/deno-require.git"
  },
  "author": "",
